generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-arm64-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Equipment {
  id              String   @id @default(cuid())
  name            String
  equipmentTypeId String
  brand           String
  model           String
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt

  @@map("equipments")
}